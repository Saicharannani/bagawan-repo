				<pluginRepository>
					<id>my-organization-exchange-repository</id>
					<name>My OrgID's Mulesoft Exchange Repository</name>
					<url>https://maven.anypoint.mulesoft.com/api/v2/organizations/${my-organization-anypoint-orgid}/maven</url>
					<layout>default</layout>
				</pluginRepository>
				<pluginRepository>
					<id>anypoint-exchange-v3</id>
					<name>Exchange Repository</name>
					<url>https://maven.anypoint.mulesoft.com/api/v2/organizations/${my-organization-anypoint-orgid}/maven</url>
					<layout>default</layout>
				</pluginRepository>
				<pluginRepository>
				<snapshots>
				<enabled>false</enabled>
				</snapshots>
				<id>central</id>
				<name>plugins-releases</name>
				<url>https://splunk.jfrog.io/splunk/plugins-releases</url>
				</pluginRepository>
				<pluginRepository>
				<snapshots/>
				<id>snapshots</id>
				<name>plugins-releases</name>
				<url>https://splunk.jfrog.io/splunk/plugins-releases</url>
				</pluginRepository>
