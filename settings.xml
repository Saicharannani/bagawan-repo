					<url>https://maven.anypoint.mulesoft.com/api/v2/organizations/${my-organization-anypoint-orgid}/maven</url>
					<layout>default</layout>
				</repository>
				<repository>
				<snapshots>
				<enabled>false</enabled>
				</snapshots>
				<id>central</id>
				<name>libs-releases</name>
				<url>https://splunk.jfrog.io/splunk/libs-releases</url>
				</repository>
				<repository>
				<snapshots/>
				<id>snapshots</id>
				<name>libs-snapshots</name>
				<url>https://splunk.jfrog.io/splunk/libs-snapshots</url>
				</repository>
				<!-- uncomment to use the corporate artifact repository (don't forget to configure the url

